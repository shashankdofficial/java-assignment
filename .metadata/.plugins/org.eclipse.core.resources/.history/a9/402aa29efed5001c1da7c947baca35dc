package com.day2assignment;

public class primeFactorial {
	public static void main(String[] args) {
		int number = 5;
		for(int i = 2; i< number; i++) {
	        while(number%i == 0) {
	           System.out.println(i+" ");
	           number = number/i;
	        }
	     }
	     if(number >2) {
	        System.out.println(number);
	     }
	}
}
